<li class = 'order_abstract_item'>
  <a href="#">
    <div class="panel-heading form_head row">
      <div class ='col-xs-4'>
        订单编号：<span class ='orderID_abstract'><%= order_abstract.orderID%></span>
      </div>
      <div class ='col-xs-4'>
        <span>
        <% if((order_abstract.status == 1)||(order_abstract.status == 2)||(order_abstract.status == 21)||(order_abstract.status == 22)||(order_abstract.status == 3)){ %>
          下单时间 ：<%= order_abstract.date %>
        <% }else if((order_abstract.status == 4)||(order_abstract.status == 41)||(order_abstract.status == 42)){%>
          发货时间 ：<%= order_abstract.shipDate %>
        <% }else if(status == 5){%>
          收货时间 ：<%= order_abstract.receiveDate %>
        <%}%>
        </span>
      </div>
      <div class="col-xs-4 text_right">
            <strong>快递点：</strong>
            <span class="label label-warning form_state"><%= order_abstract.dispatchCenter %></span>
      </div>
    </div>
  </a>
</li>