    </div>    <!-- Leftdiv Ends-->
    <div class="span2 rightdiv">
      <div class="panel panel-default" name="alert_delete">
            <div class="panel-heading">
                <h3 class="panel-title" id="urgency">信息提示栏</h3>
            </div>
            <div id="urgent_form">
              <ul class="list-group" id="danger_todo">
                <li class="list-group-item list-head" id="urgent_form_head">
                  <span class="label label-danger">急需解决</span>
                </li>
                <% for(var i = 0;i < urgentprocess.length;i++){%>
                <li class="list-group-item" id="unprocess_wholeli">
                  <a href="#" class = 'question_orderID'>订单号：<span class="question_id"><%= urgentprocess[i].orderID %></span></a><br>
                  <p class="urgentform_text">问题描述：</p>
                  <p class="urgentform_text question_description"><%= urgentprocess[i].notes %></p>
                </li>
                <%}%>
                
                <li class="list-group-item urgent-unprocess-template" style="display:none">
                  <a href="#" class = 'question_orderID'>订单号：<span class="question_id"></span></a><br>
                  <p class="urgentform_text">问题描述：</p>
                  <p class="urgentform_text question_description"></p>
                </li>

              </ul>


              <ul class="list-group" style="padding-bottom:0px" id="danger_done">
                <li class="list-group-item list-head" id="urgent_form_head">
                  <span class="label label-success">已经解决</span>
                </li>
                <% for(var i = 0;i < urgentprocessed.length;i++){%>
                <li class="list-group-item" id="processed_wholeli">
                  <a href="#" class = 'question_orderID'>订单号：<span class="question_id"><%= urgentprocessed[i].orderID %></span></a><br>
                  <p class="urgentform_text">问题描述：</p>
                  <p class="urgentform_text  question_description"><%= urgentprocessed[i].notes %></p>
                </li>
                <%}%>

                <li class="list-group-item urgent-processed-template" style="display:none">
                  <a href="#" class = 'question_orderID'>订单号：<span class="question_id"></span></a><br>
                  <p class="urgentform_text">问题描述：</p>
                  <p class="urgentform_text question_description"></p>
                </li>

              </ul>
            </div>
        </div> <!-- panel ends -->
    </div>    <!-- right div ends-->
  </div>
</div>
<!--模态框-->
<div class="modal fade" id = 'detail_modal'tabindex="-1" role="dialog"
   aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header" style="background-color:#E8A35A;">
                <button type="button" class="close"
                   data-dismiss="modal" aria-hidden="true">
                      &times;
                </button>
                <h4 class="modal-title" id="myModalLabel" style="text-align:center;">
                  <span class="alert_text">订单详情</span>
                </h4>
        </div> <!-- modal header ends-->
        <div class="modal-body">
        </div>
      </div>
  </div>
</div>

</body>
</html>